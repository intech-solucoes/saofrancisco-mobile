		"react-native": "0.57.5",
		"react-native-gesture-handler": "^1.0.9",
		"react-native-masked-text": "^1.9.2",
		"react-native-vector-icons": "^6.1.0",
		"react-native-version-number": "^0.3.5",
		"react-navigation": "^3.0.0"
	},
	"devDependencies": {
		"babel-jest": "23.6.0",
		"cpy-cli": "^2.0.0",
		"jest": "23.6.0",
		"metro-react-native-babel-preset": "0.49.2",
